--map

function draw_map()
map (0,0,0,0,128,64)
end

function check_flag(flag,x,y)
 local sprite=mget(x,y)
 return fget (sprite,flag)
end
-->8
--player

function create_player()
 p={x=6,y=8,sprite=1}
end

function player_movement()
newx=p.x
newy=p.y
 if(btnp(⬅️)) newx-=1
 if(btnp(➡️)) newx+=1
 if(btnp(⬆️)) newy-=1
 if(btnp(⬇️)) newy+=1
 
 if not  check_flag(0,newx,newy) then
  p.x=newx
  p.y=newy
  if (p.x<0) p.x=0
  if (p.y<0) p.y=0
  if (p.x>127) p.x=127
  if (p.y>63) p.y=63
 end
end
 
function draw_player()
 spr(p.sprite,p.x*8,p.y*8)
end
